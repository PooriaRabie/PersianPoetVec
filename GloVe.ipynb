    "import optuna\n",
    "from sklearn.metrics.pairwise import cosine_similarity\n",
    "import numpy as np\n",
    "import multiprocessing\n",
    "cores = multiprocessing.cpu_count() # Count the number of cores in a computer\n",
    "\n",
    "\n",
    "def evaluate_model(model):\n",
    "    \"\"\"\n",
    "    Evaluate the GloVe model based on cosine similarity between nearest neighbors.\n",
    "    \"\"\"\n",
    "    word_vectors = model.word_vectors  # Embedded vectors\n",
    "    vocab_size = word_vectors.shape[0]  # The number of words in the dictionary\n",
    "    similarities = []\n",
    "\n",
    "    for i in range(vocab_size):\n",
    "        # Current word vector\n",
    "        word_vector = word_vectors[i].reshape(1, -1)\n",
    "\n",
    "        # Calculate cosine similarity with other words\n",
    "        cos_similarities = cosine_similarity(word_vector, word_vectors).flatten()\n",
    "\n",
    "        # Similarity to the closest words (except itself)\n",
    "        nearest_neighbors = sorted(cos_similarities, reverse=True)[1:6]\n",
    "        similarities.append(np.mean(nearest_neighbors))\n",
    "\n",
    "    # Average similarity of all words\n",
    "    avg_similarity = np.mean(similarities)\n",
    "    return avg_similarity\n",
    "\n",
    "def objective(trial):\n",
    "    # Selection of hyperparameter values\n",
    "    no_components = trial.suggest_int(\"no_components\", 50, 500)\n",
    "    learning_rate = trial.suggest_float(\"learning_rate\", 0.0001, 0.5)\n",
    "    window = trial.suggest_int(\"window\", 5, 25)\n",
    "    epochs = trial.suggest_int(\"epochs\", 1, 100)\n",
    "    \n",
    "    # Create a model\n",
    "    corpus = Corpus()\n",
    "    corpus.fit(sentences, window=window)\n",
    "    model = Glove(no_components=no_components, learning_rate=learning_rate)\n",
    "    model.fit(corpus.matrix, epochs=epochs, no_threads=cores - 1, verbose=False)\n",
    "    model.add_dictionary(corpus.dictionary)\n",
    "    \n",
    "    # Assessment of model quality\n",
    "    avg_similarity = evaluate_model(model)\n",
    "    return avg_similarity\n",
    "\n",
    "\n",
    "# You can use libraries like Optuna (for Bayesian Optimization) or Scikit-Optimize to automate the hyperparameter search process.\n",
    "study = optuna.create_study(direction=\"maximize\")\n",
    "study.optimize(objective, n_trials=5) \n",
    "\n",
    "# The best hyperparameters\n",
    "print(\"Best parameters:\", study.best_params)\n"
   ]
