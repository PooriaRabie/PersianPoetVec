    "import numpy as np\n",
    "\n",
    "# Function to calculate cosine similarity\n",
    "def similarity(model, word1, word2):\n",
    "    # Check if words exist in the dictionary\n",
    "    if word1 not in model.dictionary or word2 not in model.dictionary:\n",
    "        return None  # Return None if either word is not in the dictionary\n",
    "    \n",
    "    # Get indices for the words\n",
    "    index1 = model.dictionary[word1]\n",
    "    index2 = model.dictionary[word2]\n",
    "    \n",
    "    # Fetch the vectors\n",
    "    vector1 = model.word_vectors[index1]\n",
    "    vector2 = model.word_vectors[index2]\n",
    "    \n",
    "    # Calculate cosine similarity\n",
    "    cos_sim = np.dot(vector1, vector2) / (np.linalg.norm(vector1) * np.linalg.norm(vector2))\n",
    "    return cos_sim"
   ]
